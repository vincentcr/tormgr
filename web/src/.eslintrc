{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "env" : {
    "es6": true,
    "browser": true
  },
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "rules": {
    "quotes": [2, "single"],
    "semi":1,
    "no-unused-vars":1,
    "no-undef": 2,
    "strict": 0,
    "comma-dangle": [2, "always-multiline"],
    "no-use-before-define": 0,
    "no-console": 0,
    "no-underscore-dangle": 0
  },
  "plugins": [
    "react"
  ]
}
